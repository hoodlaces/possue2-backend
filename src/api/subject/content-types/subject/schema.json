{
  "kind": "collectionType",
  "collectionName": "subjects",
  "info": {
    "singularName": "subject",
    "pluralName": "subjects",
    "displayName": "Subject",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "maxLength": 30
    },
    "description": {
      "type": "text",
      "required": true,
      "minLength": 15
    },
    "seo": {
      "type": "component",
      "repeatable": true,
      "component": "shared.seo"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "essays": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::essay.essay",
      "mappedBy": "subjects"
    }
  }
}
