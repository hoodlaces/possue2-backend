{
  "kind": "collectionType",
  "collectionName": "user_profiles",
  "info": {
    "singularName": "user-profile",
    "pluralName": "user-profiles",
    "displayName": "User Profile",
    "description": "Law student profile information"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user",
      "mappedBy": "profile"
    },
    "lawSchool": {
      "type": "string"
    },
    "lawSchoolYear": {
      "type": "enumeration",
      "required": false,
      "enum": [
        "FirstYear",
        "SecondYear",
        "ThirdYear",
        "Graduate",
        "JDCandidate",
        "Other"
      ]
    },
    "expectedGraduationDate": {
      "type": "date",
      "required": false
    },
    "currentSemester": {
      "type": "enumeration",
      "required": false,
      "enum": [
        "Fall",
        "Spring",
        "Summer"
      ]
    },
    "currentAcademicYear": {
      "type": "integer",
      "min": 2020,
      "max": 2035
    },
    "academicStanding": {
      "type": "enumeration",
      "required": false,
      "enum": [
        "TopTenPercent",
        "TopTwentyFivePercent",
        "TopFiftyPercent",
        "Other",
        "PreferNotToSay"
      ]
    },
    "gpaRange": {
      "type": "enumeration",
      "required": false,
      "enum": [
        "HighGPA",
        "AboveAverageGPA",
        "AverageGPA",
        "BelowAverageGPA",
        "PreferNotToSay"
      ]
    },
    "programType": {
      "type": "enumeration",
      "required": false,
      "enum": [
        "FullTime",
        "PartTime",
        "Evening",
        "Online",
        "Hybrid"
      ]
    },
    "targetBarExamDate": {
      "type": "date",
      "required": false
    },
    "targetBarJurisdictions": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::bar-jurisdiction.bar-jurisdiction"
    },
    "barPrepCourse": {
      "type": "enumeration",
      "required": false,
      "enum": [
        "Barbri",
        "Kaplan",
        "Themis",
        "AdaptiBar",
        "SelfStudy",
        "Undecided",
        "Other"
      ]
    },
    "barPrepTimeline": {
      "type": "enumeration",
      "required": false,
      "enum": [
        "EightPlusWeeks",
        "SixToEightWeeks",
        "FourToSixWeeks",
        "LessThanFourWeeks",
        "Undecided"
      ]
    },
    "practiceAreaInterests": {
      "type": "json"
    },
    "firmSizePreference": {
      "type": "enumeration",
      "required": false,
      "enum": [
        "BigLaw",
        "MidSize",
        "SmallFirm",
        "SoloPractice",
        "InHouse",
        "NonProfit",
        "Government"
      ]
    },
    "geographicPreference": {
      "type": "json"
    },
    "salaryExpectation": {
      "type": "enumeration",
      "required": false,
      "enum": [
        "Range50to75k",
        "Range75to100k",
        "Range100to150k",
        "Range150to200k",
        "Over200k",
        "PreferNotToSay"
      ]
    },
    "judicialClerkshipInterest": {
      "type": "enumeration",
      "required": false,
      "enum": [
        "Federal",
        "State",
        "Administrative",
        "NotInterested"
      ]
    },
    "undergraduateInstitution": {
      "type": "string",
      "required": false
    },
    "undergraduateMajor": {
      "type": "enumeration",
      "required": false,
      "enum": [
        "PreLaw",
        "PoliticalScience",
        "Business",
        "STEM",
        "LiberalArts",
        "Other"
      ]
    },
    "yearsBetweenUndergradeAndLaw": {
      "type": "enumeration",
      "required": false,
      "enum": [
        "ZeroYears",
        "OneToTwoYears",
        "ThreeToFiveYears",
        "FivePlusYears"
      ]
    },
    "previousWorkExperience": {
      "type": "enumeration",
      "required": false,
      "enum": [
        "Legal",
        "Corporate",
        "NonProfit",
        "Government",
        "Other",
        "None"
      ]
    },
    "lawReviewParticipation": {
      "type": "boolean",
      "default": false
    },
    "mootCourtParticipation": {
      "type": "boolean",
      "default": false
    },
    "studyMethod": {
      "type": "enumeration",
      "required": false,
      "enum": [
        "Visual",
        "Auditory",
        "ReadingWriting",
        "Kinesthetic",
        "Mixed"
      ]
    },
    "studySchedule": {
      "type": "enumeration",
      "required": false,
      "enum": [
        "Morning",
        "Afternoon",
        "Evening",
        "LateNight",
        "Mixed"
      ]
    },
    "studyMaterials": {
      "type": "json"
    },
    "studyGroupPreference": {
      "type": "enumeration",
      "required": false,
      "enum": [
        "Solo",
        "SmallGroups",
        "LargeGroups",
        "Mixed"
      ]
    },
    "profileCompletionPercentage": {
      "type": "integer",
      "default": 0,
      "min": 0,
      "max": 100
    },
    "marketingOptIn": {
      "type": "boolean",
      "default": false
    },
    "dataAnalyticsOptIn": {
      "type": "boolean",
      "default": false
    }
  }
}
